page{background: #ececec;}
.fabu_top{width: 750rpx;height: 322rpx;position: relative;}
.bg{width: 100%;height: 100%;}
.top_img{width: 750rpx;height: 322rpx;margin-bottom: 20rpx;background: #fff;}
