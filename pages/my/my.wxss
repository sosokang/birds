/* journalList */
.journalList {
  width: 700rpx;
  margin: 0 30rpx 0 20rpx;
  padding-bottom: 45rpx;
}
.journalList .dates {
  overflow: hidden;
  position: relative;
}
.journalList .dateDay {
  width: 60rpx;
  font-size: 30rpx;
  color: #000;
  text-align: center;
  position: absolute;
  top: 0rpx;
}
.journalList .dateMonth {
  width: 60rpx;
  font-size: 20rpx;
  color: #a2a2a2;
  text-align: center;
  position: absolute;
  top: 40rpx;
}
.journalList .dateLine {
  width: 610rpx;
  float: right;
  padding-top: 30rpx;
  padding-left: 30rpx;
  border-left: 4rpx solid #dcdcdc;
  position: relative;
}
.journalList .dateLine .details {
  background: #fff;
  padding: 5rpx;
}
.yuan {
  width: 16rpx;
  height: 16rpx;
  background: #33d1bf;
  border-radius: 50%;
  position: absolute;
  left: -10rpx;
}
.journalList .birdName {
  font-size: 22rpx;
  color: #7f7f7f;
  margin-left: 28rpx;
  margin-bottom: 15rpx;
}
.journalList .more {
  float: right;
  width: 37rpx;
  height: 6rpx;
  margin: 20rpx 26rpx 0 0;
}
.journalList .author {
  font-size: 20rpx;
  color: #7f7f7f;
