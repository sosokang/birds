}
.journalList .headUser {
  margin-left: 40rpx;
  padding-top: 95rpx;
  border-left: 4rpx solid #d2d2d2;
  position: relative;
}
.journalList .headUser:first-child {
  margin-top: 50rpx;
  padding-top: 68rpx;
} 
.journalList .headUser .headImgs .imgs {
  width: 65rpx;
  height: 65rpx;
  margin-left: -35rpx;
  margin-top: -65rpx;
  border-radius: 50%;
}
.journalList .headUser .headTitle {
  width: 600rpx;
  position: absolute;
  top: 30rpx;
  left: 45rpx;
}
.journalList .headUser:first-child .headTitle {
  top: 5rpx;
}
.headUser .headTitle .userName {
  font-size: 28rpx;
  color: #333; 
}
.headUser .headTitle .userLevel {
  width: 54rpx;
  height: 26rpx;
  font-size: 18rpx;
  color: #fff;
  background: #7faf41;
  margin-left: 15rpx;
  padding: 5rpx 10rpx;
}
.headUser .headTitle .userFocus {
  float: right;
  margin-top: 11rpx;
  font-size: 28rpx;
  color: #7faf41;
}
.journalList .headUser .imageDetails {
  /* width: 642rpx;  */
  height: 535rpx;
  margin-left: 15rpx;
  padding: 30rpx 20rpx;
  background: #fff;
  position: relative;
}
.headUser .imageDetails .summary {
  font-size: 28rpx;
  color: 333;
  line-height: 36rpx;
  /* margin-left: 22rpx; */
}
.headUser .imageDetails .img {
  width: 585rpx;
  height: 390rpx;
  margin: 30rpx 0rpx;
  position: relative;
}
.headUser .imageDetails .img .mask {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0,0.3);
    background-size: cover;
    mask-image: linear-gradient(120deg, transparent 60%, white 0%);
}
.headUser .imageDetails .img text {
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  top: 15rpx;
  right: 40rpx;
}
.headUser .imageDetails .bottomIcon view {
  display: inline-block;
  position: relative;
}
.headUser .imageDetails .bottomIcon .date{
  font-size: 20rpx;
  color: #999;
  line-height: 55rpx;margin-right: 30rpx;
} 
.headUser .imageDetails .bottomIcon image {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;margin: 0 12rpx;vertical-align: middle;
}
/* .headUser .imageDetails .bottomIcon image:first-child {
  padding-left:12rpx;
} */
.headUser .imageDetails .bottomIcon view text{
  display: inline-block;
  font-size: 22rpx;
  color: #a2a2a2;
  margin-right: 12rpx;
}
.headUser .imageDetails .bottomIcon date {
  font-size: 20rpx;
  color: #a2a2a2;
}
