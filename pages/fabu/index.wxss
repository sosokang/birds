.niao_fenlei .niao_a,.xiangqing .niao_b{width: 690rpx;height:115rpx;margin: 0 30rpx;line-height: 115rpx;color: #000;position: relative;}
.niaob{width: 690rpx;height:115rpx;position: absolute;left:0;top:0;z-index: 999;}
.fenlei_input{line-height: 115rpx;height: 115rpx;width: 300rpx;float: left;}
.hui{color: #a2a2a2;font-size: 30rpx;}
