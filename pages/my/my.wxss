  height: 42rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
}
.main-bg .topSet .message {
  float: left;
  width: 38rpx;
  height: 38rpx;
  margin-left: 40rpx;
  margin-top: 12rpx;
}
.main-bg .topSet .forward {
  float: right;
  width: 32rpx;
