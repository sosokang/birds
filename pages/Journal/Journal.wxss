.contentS .titleS {
  width: 90%;
  line-height: 44rpx;
  margin: 30rpx auto 18rpx;
  font-size: 26rpx;
  color: #333;
}
.contentS .titleS .titleTwo{
  padding: 30rpx 0 0 0;
  box-sizing: border-box;
  overflow: hidden;
  -ms-text-overflow: ellipsis;
  text-overflow: ellipsis;
  display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 5; /** 显示的行数 **/
}
.titleImg {
  width: 690rpx;
  /* height: 460rpx; */
  margin: 0 auto 20rpx;
}
.titleBr{
  display: table;
  line-height: 40rpx;
  text-align: center;
  padding: 0 22rpx;
  height: 40rpx; 
  font-size: 22rpx;
  color: #666;
  background: #ececec;
  margin: 22rpx 30rpx 20rpx 0;
}
.contentZ{
  width: 100%;
  background: #fff;
  padding:30rpx;
  box-sizing: border-box;
  border: 1px solid #ececec;
}
.zanImg{
  max-width:404rpx;
  height: 64rpx;
  border: 1px solid #fff;
  overflow: hidden;
  position: relative;
}
.zanImg image{border-radius:50%;float:left;width:64rpx;margin-left:-20rpx;}
.zanImg image:first-child{margin-left: 0;}
.zanImg::before{
  content: '.  .  .';
  text-align: center;
  line-height: 64rpx;
  font-size: 20rpx;
  color: #666;
  font-weight: bold;
  width:64rpx;
  height: 64rpx;
  border: 1px solid #fff;
  border-radius: 50%;
  background: #ececec;float: right;transform: translateX(-20rpx);
}
.zanTxt{
  font-size: 24rpx;
  color: #aeaeae;
  line-height: 1.7rem;
}
.num{
  padding: 30rpx 22rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #aeaeae;
  background: #fff;
}
.contentH{
  width: 100%;
  height: 100%;
  background: #fff;
}
.headContent{
  font-size: 24rpx;line-height: 36rpx;
