.num_box .yunsuan_num{width: 104rpx;position: relative;}
.num_box .yunsuan_num .zhi{position: absolute;right:10rpx;font-size:34rpx;
color:#a2a2a2;line-height: 52rpx;}
.yunsuan_img{width: 42rpx;height: 42rpx;margin: 36rpx 0 0 13rpx;}
.xiangqing .niao_b{border-bottom: solid 1rpx #ececec;height:95rpx;margin: 0 30rpx;line-height: 95rpx;}
