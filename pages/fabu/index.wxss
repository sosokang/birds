.btn-up{position: absolute;top: 20rpx;left:0;}
.first-child,.last-child{display: none;}
.para_text{font-size: 20rpx;color: #333;line-height: 36rpx;margin: 35rpx 35rpx 30rpx 0;height: 194rpx;overflow-y: scroll;float: left;width: 320rpx;white-space: pre-wrap;display: block;word-break:break-all;}
