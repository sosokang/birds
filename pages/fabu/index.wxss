.para{width: 700rpx;height:258rpx;margin: auto;border-radius: 10rpx;background: #fff;position: relative;}
.thumb{width: 288rpx;height:194rpx;margin:35rpx 20rpx 30rpx 30rpx;border-radius: 10rpx;background: #fff;float: left;}
.btn-del-para{width: 42rpx;height: 42rpx;position: absolute;left:-12rpx;top:-7rpx;}
