.arrow_right_grey{width: 15rpx;height: 27rpx;margin-top: 34rpx;float: right;}
/*编辑器  */
.para-adding{width:314rpx;height: 62rpx;border: solid 1rpx #d2d2d2;color: #a2a2a2;font-size: 24rpx;background: #fff;border-radius: 10rpx;margin:20rpx auto;overflow: hidden;}
.para-adding .adding-li{width:156rpx;display: inline-block;border-right: solid 1rpx #d2d2d2;line-height: 35rpx;margin-top:10rpx;}
