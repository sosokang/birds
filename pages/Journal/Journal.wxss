.dibuWrite{
  width: 750rpx;
  /* height:70rpx; */
  background: #fff;position: relative;
}
.dibuWrite input{
  width: 560rpx;
  height:70rpx;
  background: #fff;color: #333;padding-left: 30rpx;font-size: 30rpx;
}
.fabiao{position: absolute;right: 30rpx;top:0rpx;font-size: 30rpx;color: #333;z-index: 99;display: block;width: 80rpx;height: 70rpx;text-align: center;cursor: pointer;line-height: 70rpx;}
.zhuanfabox{height: 425rpx;width: 750rpx;position: fixed;bottom: 0;left:0;background: #fff;opacity: 0;z-index: 99999;}
.arrowtop{-webkit-animation: fadeInUp 1s ease 0s 1 forwards;}
@-webkit-keyframes fadeInUp{
  0%{opacity:0;-webkit-transform:translate3d(0,100%,0);transform:translate3d(0,100%,0)}
  100%{opacity:1;-webkit-transform:none;transform:none}
}
@keyframes fadeInUp{
  0%{opacity:0;-webkit-transform:translate3d(0,100%,0);-ms-transform:translate3d(0,100%,0);transform:translate3d(0,100%,0)}
  100%{opacity:1;-webkit-transform:none;-ms-transform:none;transform:none}
}
.fadeInUp{-webkit-animation-name:fadeInUp;animation-name:fadeInUp}
